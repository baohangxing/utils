{
  "name": "@bhx/utils",
  "version": "0.0.2",
  "description": "utils for web development",
  "type": "module",
  "exports": "./es/index.js",
  "typings": "./es/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "commit": "cz",
    "prepare": "husky install",
    "build": "rollup --config rollup.config.ts",
    "test": "npm run build && karma start script/karma.conf.js",
    "changelog": "conventional-changelog -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baohangxing/utils.git"
  },
  "keywords": [
    "tool",
    "util"
  ],
  "files": [
    "es/",
    "lib/",
    ".md"
  ],
  "author": "baohangxing",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "commitizen": "^4.2.5",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.21.0",
    "eslint-config-alloy": "^4.6.2",
    "husky": "^8.0.1",
    "jasmine-core": "^4.3.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-typescript2": "^0.32.1",
    "typescript": "^4.7.4"
  },
  "bugs": {
    "url": "https://github.com/baohangxing/utils/issues"
  },
  "homepage": "https://github.com/baohangxing/utils#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
